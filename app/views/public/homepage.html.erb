<h1 class="mt-5">Welcome to InstaRails</h1>
        <p class="lead">Please Sign in or register</p>
        <ul class="list-unstyled">
          <li>To continue</li>
         
        </ul>

<% if current_user %>
  Logged in as <%= current_user.user_name %>
  <%= link_to "Log Out", logout_path, class: 'btn btn-dark' %>
<% else %>
  <%= link_to "Sign Up", signup_path, class: 'btn btn-primary' %> or
  <%= link_to "Log In", login_path, class: 'btn btn-success' %>


<% end %>

